<nav>
  <ul class="pager">
    <% if (page.prev) { %>
      <li>
        <a href="<%- config.root %><%- page.prev_link %>  ">
          Previous
        </a>
      </li>
    <% } else { %>
      <!-- <li>Previous</li> -->
    <% } %>

    <% if (page.next) { %>
      <li>
        <a href="<%- config.root %><%- page.next_link %>  ">
          Next
        </a>
      </li>
    <% } else { %>
      <!-- <li>Next</li> -->
    <% } %>
  </ul>
</nav>